{
    "Name": "ConsortiumSandboxQReadOnly",
    "Description": "AmazonQ Read Only Access",
    "SessionDuration": "PT1H",
    "ManagedPolicies": [],
    "CustomPolicy": {
        "Version": "2012-10-17",
        "Statement": [
            {
                "Sid": "AmazonQReadOnlyMinimalAccess",
                "Effect": "Allow",
                "Action": [
                    "q:ListPluginProviders",
                    "q:ListDashboardMetrics",
                    "q:ListPlugins",
                    "q:ListTagsForResource",
                    "q:ListConversations"
			],
                "Resource": "*"
            }
        ]
    }
}